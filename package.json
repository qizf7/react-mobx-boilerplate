{
  "name": "react-mobx-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "clean": "rimraf dist/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js",
    "build:dll": "webpack --config build/webpack.dll.config.js --progress --hide-modules --display-optimization-bailout",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "mock": "nodemon --watch mock build/mock-server.js",
    "lint": "eslint --ext .js --ext .jsx --fix src/"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "qizf",
  "license": "MIT",
